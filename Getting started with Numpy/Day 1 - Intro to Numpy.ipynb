      "[[8 8 6 2 8 7]]\n",
      "[[  8   8 222   2   8   7]]\n"
     ]
    }
   ],
   "source": [
    "X = arr.copy()\n",
    "Y = arr.view()\n",
    "print(arr)\n",
    "arr[0,2] = 222\n",
    "print(arr)\n",
    "print(X)\n",
    "print(Y)"
   ]
